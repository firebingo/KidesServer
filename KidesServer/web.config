  </system.webServer>
  <system.web>
    <globalization fileEncoding="utf-8" requestEncoding="utf-8" responseEncoding="utf-8" culture="en-US" uiCulture="en-US" />
  </system.web>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="bin\x64\Debug\net5.0\win10-x64\KidesServer.exe" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" arguments="" hostingModel="inprocess">
        <environmentVariables />
      </aspNetCore>
    </system.webServer>
  </location>
